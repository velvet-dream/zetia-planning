{# templates/project/_delete_form.html.twig #}
<form method="post" action="{{ path('app_project_delete', {'pctId': project.pctId}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet élément ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ project.pctId) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>