{# Default form #}
<div class="zetia-form-container">
	{{ form_errors(form) }}
	{{ form_start(form) }}
	{{ form_widget(form, {'attr': {'class': 'zetia-form-inputs-container'}}) }}
	{% set buttonVariant = button_variant ?? "purple"  %}
	<twig:Button variant="{{ buttonVariant }}" text="{{ button_label|default('Enregistrer') }}"/>
	{{ form_end(form) }}
</div>
